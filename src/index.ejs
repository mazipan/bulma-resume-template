                    </div>
                </div>
            </div>
        </div>
    </section>

    <div class="hr"></div>

    <section class="section">
        <div class="container">
            <div class="columns">
                <div class="column is-three-fifths">
                    <div class="summary">
                        <div class="title is-size-5 has-text-primary has-text-weight-bold">SUMMARY</div>
                        <div class="wrapper">
                            <%= content.summary %>
                        </div>
                    </div>

                    <div class="experience">
                        <div class="title is-size-5 has-text-primary has-text-weight-bold">EXPERIENCE</div>

                        <% content.experiences.forEach(function(experience){ %>
                            <div class="item">
                                <div class="is-size-5">
                                    <b><%= experience.company %>,</b>
                                    <span class="has-text-weight-semi-bold">
                                        <%= experience.location %> — <i><%= experience.title %></i>
                                    </span>
                                </div>
                                <div class="is-size-7"><%= experience.period %></div>
                                <% if (experience.jobdesc.length > 0) { %>
                                    <ul style="list-style: disc;margin-left: 1em;">
                                        <% experience.jobdesc.forEach(function(job){ %>
                                            <li><%= job %></li>
                                        <% }); %>
                                    </ul>
                                <% } %>
                            </div>
                        <% }); %>

                    </div>

                    <div class="education">
                        <div class="title is-size-5 has-text-primary has-text-weight-bold">EDUCATION</div>

                        <% content.educations.forEach(function(education){ %>
                            <div class="item">
                                <div class="is-size-5">
                                    <b><%= education.name %></b>,
                                    <span class="has-text-weight-semi-bold">
                                        <%= education.city %> — <i><%= education.degree %></i>
                                    </span>
                                </div>
                                <div class="is-size-7"><%= education.period %></div>
                                <div><%= education.faculty %></div>
                            </div>
                        <% }); %>
                    </div>

                </div>
                <div class="column">
                    <div class="projects">
                        <div class="title is-size-5 has-text-primary has-text-weight-bold">PROJECTS</div>

                        <% content.projects.forEach(function(proj){ %>
                            <div class="item">
                                <div class="is-size-5">
                                    <b><%= proj.title %></b>
                                     —
                                    <i><%= proj.company %></i>
                                </div>
                                <div class="is-size-7"><%= proj.period %></div>
                            </div>
                        <% }); %>

                    </div>

                    <div class="skills">
                        <div class="title is-size-5 has-text-primary has-text-weight-bold">SKILLS</div>
                        <div class="wrapper">
                            <% content.skills.forEach(function(skill){ %>
                                <div class="item">
                                    <div><%= skill.name %></div>
                                    <progress class="progress is-primary is-small" value="<%= skill.progress %>" max="100"><%= skill.progress %>%</progress>
                                </div>
                            <% }); %>
                        </div>
                    </div>
                    <div class="languages">
                        <div class="title is-size-5 has-text-primary has-text-weight-bold">LANGUAGES</div>
                        <div class="wrapper">
                            <% content.languages.forEach(function(lang){ %>
                                <%= lang %>,
                            <% }); %>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>

